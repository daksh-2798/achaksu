
@{
    ViewBag.Title = "AdminAddCategory";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}

<link href="~/bossCss/bootstrap.min.css" rel="stylesheet" />
@using (Html.BeginForm("AdminAddCategory", "Admin", FormMethod.Post))
{
    <div class="container" style="margin-top:30px">
        <div class="col-md-8">
            <div class="panel panel-default">
                <div class="panel-heading"><h3 class="panel-title"><strong>Add Category </strong></h3></div>
                <div class="panel-body">
                    <form role="form">

                        <div class="form-group">
                            <label for="exampleInputCatId">Category Id</label>
                            <input type="text" class="form-control" id="exampleInputCatId" name="catid" placeholder="Enter Category Id">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputCatType">Category Type</label>
                            <input type="text" class="form-control" id="exampleInputCatType" name="ctype" placeholder="Enter Category Type">
                        </div>
                        <button type="submit" class="btn btn-sm btn-default">Add Category</button>
                    </form>
                </div>
            </div>
        </div>

    </div>

}
